{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "//#install-rust": {
      "cache": true,
      "outputs": [".cargo/**"],
      "env": ["CARGO_HOME=.cargo"]
    },
    "parser-js-wrapper#build": {
      "dependsOn": ["//#install-rust"],
      "outputs": ["pkg/**"],
      "env": ["CARGO_HOME=.cargo"]
    },
    "install": {
      "dependsOn": ["parser-js-wrapper#build"],
      "outputs": ["node_modules/**", "pnpm-lock.json"]
    },
    "build": {
      "dependsOn": ["parser-js-wrapper#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {},
    "lint": {},
    "test": {}
  }
}
